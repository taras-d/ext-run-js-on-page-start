{
  "name": "RunJsOnPageStart",
  "description": "Run javascript code before page starts loading",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_start"
  }]
}